{
  "name": "bydelsfakta",
  "version": "1.1.50",
  "private": true,
  "scripts": {
    "dev": "vite",
    "serve": "npm run dev",
    "backend": "env-cmd -f ./.env.local nodemon server/server.js",
    "build": "vite build --mode production",
    "build:prod": "npm install && npm run lint && npm run test:unit && npm run build",
    "docker": "npm run build:prod && npm run docker:build",
    "docker:build": "docker build -t ghcr.io/oslokommune/bydelsfakta-frontend:$npm_package_version .",
    "docker:push": "docker push ghcr.io/oslokommune/bydelsfakta-frontend:$npm_package_version",
    "lint": "eslint ./src ./server --ext .js,.vue",
    "lint:style": "stylelint 'src/**/*.scss' 'src/**/*.vue'",
    "lint:style:fix": "stylelint 'src/**/*.scss' 'src/**/*.vue' --fix",
    "test:unit": "NODE_ENV=test jest",
    "test:unit:watch": "NODE_ENV=test jest --watchAll"
  },
  "engines": {
    "node": "16"
  },
  "browserslist": {
    "production": [
      ">0.1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@oslokommune/punkt-assets": "^3.1.1",
    "@oslokommune/punkt-vue2": "^3.1.1",
    "@sentry/vue": "^6.7.2",
    "@xmldom/xmldom": "^0.8.6",
    "autoprefixer": "^10.4.14",
    "axios": "^0.21.2",
    "canvas": "^2.8.0",
    "canvg": "^3.0.7",
    "core-js": "^3.15.1",
    "d3": "^5.16.0",
    "d3-svg-legend": "^2.25.6",
    "d3-textwrap": "^3.0.0",
    "leaflet": "^1.7.1",
    "leaflet-gesture-handling": "^1.2.1",
    "nodemon": "^2.0.20",
    "save-svg-as-png": "^1.4.17",
    "vue": "^2.7.14",
    "vue-analytics": "^5.22.1",
    "vue-dragscroll": "^1.10.2",
    "vue-i18n": "^8.24.5",
    "vue-meta": "^2.4.0",
    "vue-resize": "^1.0.1",
    "vue-router": "^3.5.2",
    "vue-skip-to": "^1.0.6",
    "vue2-leaflet": "^2.7.0",
    "vuex": "^3.6.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/test-utils": "^1.2.1",
    "@vue/vue2-jest": "^27.0.0",
    "babel-eslint": "^10.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "stylelint": "^15.4.0",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-recommended-scss": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^4.6.0",
    "vite": "^4.2.1",
    "vue-template-compiler": "^2.7.14"
  }
}
